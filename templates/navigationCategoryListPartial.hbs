<nav>
    <ul>
        {{#each (lookup . categoryList)}}
        {{#if (and (lookup .. "isCategoryIndex") (equal category (lookup .. "category")))}}
        <li>{{category}}</li>
        {{else}}
        <li><a href="{{../rootPath}}posts/{{category}}">{{category}}</a></li>
        {{/if}}
        {{/each}}
        {{#if (equal categoryList "categories_top")}}
        <li><a href="{{../rootPath}}archive.html">...</a></li>
        {{/if}}
    </ul>
</nav>